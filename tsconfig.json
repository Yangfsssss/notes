{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./dist/",
    "experimentalDecorators": true,
    "allowJs": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "paths": {
      "pages/*":[
        "./State Manage in React/code/src/pages/*"
      ],
      "@utils/*":[
        "./State Manage in React/code/src/utils/*"
      ],
    },      
  }
}
